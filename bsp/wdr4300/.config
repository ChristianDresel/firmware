# Freifunk Franken base
CONFIG_PACKAGE_fff-base=y

# Needed to set busybox features
CONFIG_BUSYBOX_CUSTOM=y

# Alfred
CONFIG_PACKAGE_alfred=y
CONFIG_PACKAGE_ALFRED_VIS=n

# Fastd
CONFIG_PACKAGE_fastd=y
CONFIG_FASTD_ENABLE_CIPHER_SALSA2012=n
CONFIG_FASTD_ENABLE_MAC_GHASH=n
CONFIG_FASTD_ENABLE_METHOD_COMPOSED_GMAC=n
CONFIG_FASTD_ENABLE_METHOD_GENERIC_GMAC=n

# ip and bridge command
CONFIG_PACKAGE_ip=y
# We need the real bridge, so remove busybox bridge
CONFIG_PACKAGE_ip-bridge=y
CONFIG_BUSYBOX_CONFIG_BRCTL=n

# Broadcast filter
CONFIG_PACKAGE_ebtables=y
CONFIG_PACKAGE_ebtables-utils=y
CONFIG_PACKAGE_kmod-ebtables-ipv4=y
CONFIG_PACKAGE_kmod-ebtables-ipv6=y
CONFIG_PACKAGE_arptables=y

# Opt. Bandwith limits
CONFIG_PACKAGE_qos-scripts=y

# Batman Adv legacy
CONFIG_PACKAGE_kmod-batman-adv-legacy=y
CONFIG_KMOD_BATMAN_ADV_LEGACY_NC=y

# Countries for Atheros
CONFIG_ATH_USER_REGD=y

# Not used OpenWRT things
CONFIG_PACKAGE_ppp=n
CONFIG_PACKAGE_opkg=n
CONFIG_PACKAGE_odhcpd=n
CONFIG_PACKAGE_dnsmasq=n
CONFIG_PACKAGE_firewall=n

# Unknown
CONFIG_PACKAGE_wireless-tools=y
CONFIG_PACKAGE_libugpio=y
CONFIG_PACKAGE_gpioctl-sysfs=y
CONFIG_PACKAGE_iptables-mod-filter=y
CONFIG_CLEAN_IPKG=y
CONFIG_BUSYBOX_CONFIG_FEATURE_WGET_TIMEOUT=y
CONFIG_BUSYBOX_CONFIG_HTTPD=y
CONFIG_BUSYBOX_CONFIG_FEATURE_NTPD_SERVER=n

# special LED config
CONFIG_PACKAGE_kmod-ledtrig-heartbeat=y
CONFIG_PACKAGE_kmod-ledtrig-netdev=y

# WDR4300 router
CONFIG_TARGET_ar71xx=y
CONFIG_TARGET_ar71xx_generic=y
CONFIG_TARGET_ar71xx_generic_TLWDR4300=y

