        option 'ipv4_address' '1'
	option 'ipv6_interface' 'br-mesh'
	option 'ipv6_address' 'fe80::201:2ff:fe03:405'
        option 'timeout' '5'
        option 'retry' '5'

config 'update'
        option 'autoupdate' '1'

config 'network'
	option 'mesh_interface' 'br-mesh'
	option 'client_interfaces' 'ath0 wlan0 eth0.1'
